<?xml version="1.0" encoding="utf-8"?>
<MasterDetailPage 
		xmlns="http://xamarin.com/schemas/2014/forms" 
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
		xmlns:local="clr-namespace:Priceless" 
		x:Class="Priceless.MasterDetail"
		MasterBehavior="Popover"
		>
	<MasterDetailPage.Master>
		<local:Master x:Name="master" />
	</MasterDetailPage.Master>
	<MasterDetailPage.Detail>
		<NavigationPage Title="Priceless" Tint="Fuchsia" BarBackgroundColor="#E91E63" BarTextColor="White" >
			<x:Arguments>
				<local:Abas Title="Priceless" />
			</x:Arguments>
		</NavigationPage>
	</MasterDetailPage.Detail>
</MasterDetailPage>